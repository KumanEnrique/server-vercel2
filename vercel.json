{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "cleanUrls": true,
    
    "routes":[
        {
            "src":"/",
            "dest":"/index.js",
            "methods":["GET"]
        },
        {
            "src":"/api",
            "dest":"/index.js",
            "methods":["GET"]
        },
        {
            "src":"/users",
            "dest":"/index.js",
            "methods":["GET"]
        }
    ]
}
